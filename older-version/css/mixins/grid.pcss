@define-mixin grid-span $class, $rule {
  $(class)-auto {
    $(rule): auto;
  }
  
  $(class)-1 {
    $(rule): span 1 / span 1;
  }
  
  $(class)-2 {
    $(rule): span 2 / span 2;
  }
  
  $(class)-3 {
    $(rule): span 3 / span 3;
  }
  
  $(class)-4 {
    $(rule): span 4 / span 4;
  }
  
  $(class)-5 {
    $(rule): span 5 / span 5;
  }
  
  $(class)-6 {
    $(rule): span 6 / span 6;
  }
  
  $(class)-7 {
    $(rule): span 7 / span 7;
  }
  
  $(class)-8 {
    $(rule): span 8 / span 8;
  }
  
  $(class)-9 {
    $(rule): span 9 / span 9;
  }
  
  $(class)-10 {
    $(rule): span 10 / span 10;
  }
  
  $(class)-11 {
    $(rule): span 11 / span 11;
  }
  
  $(class)-12 {
    $(rule): span 12 / span 12;
  }
  
  $(class)-full {
    $(rule): 1 / -1;
  }
}


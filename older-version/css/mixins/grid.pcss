@define-mixin grid-mixin $class, $rule {
  $(class)-1 {
    display: grid;
    $(rule): repeat(1, minmax(0, 1fr));
  }
  
  $(class)-2 {
    display: grid;
    $(rule): repeat(2, minmax(0, 1fr));
  }
  
  $(class)-3 {
    display: grid;
    $(rule): repeat(3, minmax(0, 1fr));
  }
  
  $(class)-4 {
    display: grid;
    $(rule): repeat(4, minmax(0, 1fr));
  }
  
  $(class)-5 {
    display: grid;
    $(rule): repeat(5, minmax(0, 1fr));
  }
  
  $(class)-6 {
    display: grid;
    $(rule): repeat(6, minmax(0, 1fr));
  }
  
  $(class)-7 {
    display: grid;
    $(rule): repeat(7, minmax(0, 1fr));
  }
  
  $(class)-8 {
    display: grid;
    $(rule): repeat(8, minmax(0, 1fr));
  }
  
  $(class)-9 {
    display: grid;
    $(rule): repeat(9, minmax(0, 1fr));
  }
  
  $(class)-10 {
    display: grid;
    $(rule): repeat(10, minmax(0, 1fr));
  }
  
  $(class)-11 {
    display: grid;
    $(rule): repeat(11, minmax(0, 1fr));
  }
  
  $(class)-12 {
    display: grid;
    $(rule): repeat(12, minmax(0, 1fr));
  }
  
  $(class)-none {
    display: grid;
    $(rule): none;
  }
}


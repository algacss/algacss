@alga base {
  *, ::after, ::before {
    ref: boxSizing-borderBox;
  }
  
  body {
    ref: margin-0px padding-0px;
    ref-fontFamily: 'Calibri', Arial;
  }
  
  .h1, .h2, .h3, .h4, .h5, .h6, 
  h1, h2, h3, h4, h5, h6 {
    ref: marginTop-0px marginBottom-0.5rem fontWeight-500 lineHeight-1.2;
  }
  
  .mark, mark {
    ref: backgroundColor-hex707070 borderRadius-0.25rem color-hexfff;
    ref-padding: .1875em .375rem;
  }
}
